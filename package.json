{
  "name": "reviews_component_fec",
  "version": "1.0.0",
  "description": "This is the Reviews component for our TripAdvisor clone for our Front-End Capstone. The backend tests scaling",
  "author": "Dmitri Tarasov(server optimization) & Gabor Szekely(Front-end components)",
  "license": "MIT",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run build\"",
    "startdocker": "concurrently \"npm run server\" \"npm run dockerbuild\"",
    "docker": "concurrently \"npm run seed\" \"npm run server\"",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production --watch",
    "dockerbuild": "webpack --mode production",
    "seed": "\"cd ./server/postgresmodels && ./copyfiles.bash && docker exec -it friendly_meitner bash chmod 777 launchsql.bash && docker exec -it friendly_meitner bash ./launchsql.bash\"",
    "server": "nodemon server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "csv-stringify": "^5.3.0",
    "dateformat": "^3.0.3",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "knex": "^0.16.5",
    "mongoose": "^5.4.20",
    "newrelic": "^5.7.0",
    "pg": "^7.10.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-loading-skeleton": "^1.1.1",
    "redis": "^2.8.0",
    "whatwg-fetch": "^3.0.0"
  },
